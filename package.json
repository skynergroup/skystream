{
  "name": "skystream",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:staging": "NODE_ENV=staging vite build",
    "build:production": "NODE_ENV=production vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 10",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "type-check": "echo 'TypeScript not configured yet'",
    "test": "echo 'Tests not configured yet'",
    "test:unit": "echo 'Unit tests not configured yet'",
    "test:e2e": "echo 'E2E tests not configured yet'",
    "test:ci": "npm run test:unit && npm run test:e2e",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "verify-build": "node scripts/verify-build.js",
    "postbuild": "npm run verify-build"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.840.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.840.0",
    "amazon-cognito-identity-js": "^6.3.15",
    "buffer": "^6.0.3",
    "lucide-react": "^0.523.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx-self": "^7.27.1",
    "@babel/plugin-transform-react-jsx-source": "^7.27.1",
    "@eslint/js": "^9.29.0",
    "@locator/babel-jsx": "^0.4.4",
    "@locator/runtime": "^0.4.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "prettier": "^3.2.5",
    "terser": "^5.43.1",
    "vite": "^7.0.0"
  }
}
